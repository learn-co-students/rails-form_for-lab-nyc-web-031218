<h1>Edit <%= @school_class.title %></h1>

<%= form_for @school_class do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br />
  <%= f.label :room_number %>
  <%= f.text_field :room_number %><br />
  <%= f.submit %>
<% end %>

<hr />
<%= link_to "All School Classes", school_classes_path %>
